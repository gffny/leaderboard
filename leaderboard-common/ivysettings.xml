<?xml version="1.0" encoding="UTF-8"?>
<ivysettings>
  <settings defaultResolver="custom"/>
  <include url="${ivy.default.settings.dir}/ivysettings-public.xml"/>
  <include url="${ivy.default.settings.dir}/ivysettings-shared.xml"/>
  <include url="${ivy.default.settings.dir}/ivysettings-local.xml"/>
  <include url="${ivy.default.settings.dir}/ivysettings-main-chain.xml"/>
  <include url="${ivy.default.settings.dir}/ivysettings-default-chain.xml"/>
  <resolvers>
    <filesystem name="third-party-dependencies" local="true" changingPattern=".*SNAPSHOT" checkmodified="true">
      <ivy pattern="${wkspace.dir}/third-party-dependencies/[organisation]/[module]/[revision]/[artifact].[ext]" />
      <artifact pattern="${wkspace.dir}/third-party-dependencies/[organisation]/[module]/[revision]/[artifact].[ext]" />
    </filesystem>
    <chain name="custom" changingPattern=".*SNAPSHOT">
      <resolver ref="third-party-dependencies"/>
      <ibiblio name="files.couchbase.com" m2compatible="true" root="http://files.couchbase.com/maven2/" />
      <ibiblio name="maven.scale7.org" m2compatible="true" root="https://github.com/s7/mvnrepo/raw/master" />
      <ibiblio name="spring.milestones" m2compatible="true" root="http://s3.amazonaws.com/maven.springframework.org/milestone" />
      <resolver ref="main"/>
    </chain>
  </resolvers>
</ivysettings>

