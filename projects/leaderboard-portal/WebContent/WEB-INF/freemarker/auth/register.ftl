<h1>Leaderboard Registration</h1>
<form action="/leaderboard-portal/authentication/register" method="POST">
	<label for="username">Profile Handle (username):</label>
	<input id="username" name="username" type="text"/>
	<label for="email">eMail Address:</label>
	<input id="email" name="email" type="text"/>
	<label for="confirmEmail">Confirm eMail Address:</label>
	<input id="confirmEmail" name="confirmEmail" type="text"/>
	<label for="firstName">First Name:</label>
	<input id="firstName" name="firstName" type="text"/>
	<label for="lastName">Last Name:</label>
	<input id="lastName" name="lastName" type="text"/>
	<label for="handicap">Handicap:</label>
	<input id="handicap" name="handicap" type="text"/>
	<label for="password">Handicap:</label>
	<input id="password" name="password" type="password"/>
	<input type="submit" value="continue"/>
</form>